{
    "name": "decodelabs/compass",
    "description": "Parse, inspect and test IP addresses",
    "type": "library",
    "keywords": [
        "ip",
        "address"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        }
    ],
    "require": {
        "php": "^8.4",

        "decodelabs/exceptional": "^0.6.1",
        "decodelabs/fluidity": "^0.3.6",
        "decodelabs/nuance": "^0.2",

        "brick/math": "^0.12|^0.13"
    },
    "require-dev": {
        "decodelabs/chorus": "^0.1",
        "decodelabs/lucid": "^0.8",
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "conflict": {
        "decodelabs/lucid": "<0.8"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Compass\\": "src/Compass/"
        },
        "classmap": [
            "src/Lucid/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.4.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
