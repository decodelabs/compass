{
    "name": "decodelabs/compass",
    "description": "Parse, inspect and test IP addresses",
    "type": "library",
    "keywords": ["ip", "address"],
    "license": "MIT",
    "authors": [{
        "name": "Tom Wright",
        "email": "tom@inflatablecookie.com"
    }],
    "require": {
        "php": "^8.0",

        "decodelabs/exceptional": "^0.4",
        "decodelabs/fluidity": "^0.3.2",
        "decodelabs/glitch-support": "^0.4",

        "brick/math": "^0.10.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1",
        "phpstan/extension-installer": "^1.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "symplify/easy-coding-standard": "^11",

        "decodelabs/phpstan-decodelabs": "^0.6"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Compass\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
