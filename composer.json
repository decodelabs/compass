{
    "name": "decodelabs/compass",
    "description": "Parse, inspect and test IP addresses",
    "type": "library",
    "keywords": [ "ip", "address" ],
    "license": "MIT",
    "authors": [ {
            "name": "Tom Wright",
            "email": "tom@inflatablecookie.com"
        } ],
    "require": {
        "php": "^8.4",

        "decodelabs/exceptional": "^0.5.2",
        "decodelabs/fluidity": "^0.3.6",
        "decodelabs/glitch-support": "^0.5.1",

        "brick/math": "^0.12.1|^0.12.1|^0.12.1"
    },
    "require-dev": {
        "decodelabs/phpstan-decodelabs": "^0.7"
    },
    "autoload": {
        "psr-4": {
            "DecodeLabs\\Compass\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.3.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
